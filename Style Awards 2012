<script type="text/javascript">
$(document).ready(function() {

 if ($.browser.msie && $.browser.version.substr(0,1)<8){
 $('#beauty-menu li').has('ul').mouseover(function(){
            $(this).addClass("hover");
$(this).children('ul').css('visibility','visible');
            }).mouseout(function(){
            $(this).removeClass("hover");
 $(this).children('ul').css('visibility','hidden');
            });
}

$('ul.inner').hide();
$('.beauty-feature>div').hover(function(){
$(this).addClass('hover').find('ul.inner').slideDown('fast');
$(this).hover(function(){},function(){
$(this).removeClass('hover').find('ul.inner').slideUp('fast');
});
return false;
});
}); 
</script>
